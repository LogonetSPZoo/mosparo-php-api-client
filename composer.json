{
    "name": "mosparo/php-api-client",
    "description": "PHP API Client to communicate with mosparo.",
    "keywords": ["mosparo", "php", "client"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mosparo",
            "homepage": "https://mosparo.io"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Mosparo\\ApiClient\\": "src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
